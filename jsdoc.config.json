{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include":[ "base", "consys", "grabber", "lul" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",               
        "destination": "documentation",          
        "recurse": true,                  
        "readme": "tutorials/README.md",
        "tutorials": "tutorials"
    }
}


